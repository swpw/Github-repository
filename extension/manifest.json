{
  "manifest_version": 2,
  "name": "Github Repos",
  "version": "1.0.0",
  "description": "Quickly look up github repositories in simple popup menu.",
  "homepage_url": "https://github.com/swpw/Github-repository",
  "icons": {
    "128": "./icons/icon128.png",
    "48": "./icons/icon32.png",
    "16": "./icons/icon16.png"
  },
  "browser_action": {
    "default_icon": "./icons/icon16.png",
    "default_popup": "./popup.html"
  },
  "permissions": [
    "storage"
  ]
}
